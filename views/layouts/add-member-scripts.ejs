            <script type="text/javascript">
                $('#sl').click(function() {
                    $('#tl').loadingBtn();
                    $('#tb').loadingBtn({
                        text: "Signing In"
                    });
                });

                $('#el').click(function() {
                    $('#tl').loadingBtnComplete();
                    $('#tb').loadingBtnComplete({
                        html: "Sign In"
                    });
                });

                $('#demoDate').datepicker({
                    format: "dd/mm/yyyy",
                    autoclose: true,
                    todayHighlight: true
                });

                $('#demoSelect').select2();
            </script>
